{
  "name": "chatterbox-desktop",
  "version": "1.0.43",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "electron-dl": "^3.0.1",
    "electron-json-storage": "^4.1.8",
    "electron-log": "^4.2.2",
    "electron-updater": "^4.3.1",
    "firebase": "^7.15.0",
    "foreman": "^3.0.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5"
  },
  "main": "src/electron-starter.js",
  "scripts": {
    "react-start": "react-app-rewired start",
    "react-build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "start": "nf start",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build:osx": "electron-builder --mac",
    "build:linux": "npm run build:linux32 && npm run build:linux64",
    "build:linux32": "electron-builder --linux --ia32",
    "build:linux64": "electron-builder --linux --x64",
    "build:win": "npm run build:win32 && npm run build:win64",
    "build:win32": "electron-builder --win --ia32",
    "build:win64": "electron-builder --win --x64",
    "build": "electron-builder build --mac --win --publish never",
    "deploy": "electron-builder build --mac --win --publish always"
  },
  "homepage": ".",
  "build": {
    "productName": "SmartLog Desktop",
    "appId": "com.smlog.chatterbox",
    "asar": true,
    "extends": null,
    "protocols": {
      "name": "smlog",
      "schemes": [
        "smlog"
      ]
    },
    "mac": {
      "target": [
        "default"
      ],
      "icon": "./public/icon01_512.png"
    },
    "dmg": {
      "title": "SmartLog",
      "icon": "./public/icon01_512.png"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./public/icon01_256.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "zip",
        "tar.gz"
      ]
    },
    "directories": {
      "buildResources": "resources/installer/",
      "output": "dist/",
      "app": "."
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.0.3",
    "electron-builder": "^22.7.0",
    "electron-rebuild": "^1.11.0",
    "react-app-rewired": "^2.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gitsimu/chatterbox-desktop.git"
  }
}
